{
  "name": "nextjs14-typescript-app-router-contentful",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "cf-export": "source .env.local && contentful space export --config contentfulsync/export/config/export-config.json --management-token $CONTENTFUL_MANAGEMENT_TOKEN --space-id $CONTENTFUL_SPACE_ID",
    "cf-model-export": "source .env.local && contentful space export --config contentfulsync/export/config/export-config-contentmodel.json --management-token $CONTENTFUL_MANAGEMENT_TOKEN --space-id $CONTENTFUL_SPACE_ID",
    "cf-import": "source .env.local && contentful space import --config contentfulsync/import/config/contentful-import-config.json --management-token $CONTENTFUL_MANAGEMENT_TOKEN --space-id $CONTENTFUL_SPACE_ID",
    "graphql-codegen:generate": "graphql-codegen -r dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "graphql-codegen:watch": "graphql-codegen --watch -r dotenv/config --config codegen.ts dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^16.0.13",
    "@contentful/rich-text-react-renderer": "^15.19.6",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@prisma/adapter-neon": "^5.14.0",
    "@prisma/client": "^5.14.0",
    "@tailwindcss/typography": "github:tailwindcss/typography",
    "@tremor/react": "^3.17.2",
    "@types/xml2js": "^0.4.14",
    "@upstash/kafka": "^1.3.5",
    "@upstash/ratelimit": "^1.1.3",
    "@upstash/redis": "^1.31.3",
    "algoliasearch": "^4.23.3",
    "contentful-cli": "^3.1.44",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "instantsearch.css": "^8.1.0",
    "next": "^14.1.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "prisma": "^5.14.0",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.2.1",
    "react-instantsearch": "^7.7.1",
    "react-instantsearch-nextjs": "^0.2.0",
    "react-instantsearch-router-nextjs": "^7.7.1",
    "react-intersection-observer": "^9.10.3",
    "react-medium-image-zoom": "^5.2.0",
    "react-toastify": "^10.0.5",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.2",
    "tailwind-merge": "^2.2.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
